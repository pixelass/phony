{
  "all": true,
  "check-coverage": true,
  "exclude": [
    "src/utils/types.ts",
    "src/export.ts",
    "src/flush.ts",
    "src/index.ts",
    "src/constants.ts",
    "src/utils/database.ts",
    "src/utils/root.ts",
    "src/utils/schema.ts",
    "src/utils/serve.ts"
  ],
  "extension": [
    ".js",
    ".ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "report-dir": "coverage",
  "reporter": [
    "html",
    "lcov",
    "text",
    "text-summary"
  ]
}
